{
  "name": "entitizer-free-api",
  "version": "0.1.1",
  "private": true,
  "description": "Entitizer free API",
  "main": "./lib/app.js",
  "scripts": {
    "start": "tsc && node ./lib/app.js",
    "build": "tsc",
    "pretest": "npm run clean && npm run tsc",
    "test": "ava --no-cache --verbose lib/**/*.test.js",
    "posttest": "npm run clean-test-code",
    "tsc": "tsc",
    "tscw": "tsc -w",
    "preversion": "npm run tsc",
    "postversion": "git push --follow-tags",
    "prepublishOnly": "npm run tsc && npm run clean-test-code",
    "clean": "rimraf ./lib && rimraf ./types",
    "clean-test-code": "rimraf ./lib/**/*.test.js && rimraf ./types/**/*.test.d.ts"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://Mitea@bitbucket.org/entitizer/free-api-js.git"
  },
  "keywords": [
    "entitizer",
    "textactor",
    "app",
    "free-api",
    "api"
  ],
  "author": "Dumitru Cantea",
  "license": "ISC",
  "bugs": {
    "url": "https://Mitea@bitbucket.org/entitizer/free-api-js/issues"
  },
  "homepage": "https://Mitea@bitbucket.org/entitizer/free-api-js#readme",
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/lru-cache": "^4.1.1",
    "@types/mongodb": "^3.1.14",
    "@types/node": "^10.12.6",
    "ava": "^0.25.0",
    "debug": "^4.1.0",
    "dotenv": "^6.1.0",
    "rimraf": "^2.6.2",
    "typescript": "3.1.6"
  },
  "dependencies": {
    "@textactor/actor-data": "^0.2.0",
    "@textactor/concept-data": "^0.2.0",
    "@textactor/concept-domain": "^0.2.0",
    "@textactor/domain": "^0.2.0",
    "@textactor/ner": "^0.2.1",
    "@textactor/wikientity-data": "^0.2.0",
    "@textactor/wikientity-domain": "^0.2.0",
    "aws-sdk": "^2.354.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-rate-limit": "^3.3.2",
    "lru-cache": "^4.1.3",
    "mongodb": "^3.1.9",
    "ms": "^2.1.1",
    "ournet.logger": "^0.1.0"
  }
}
