{
  "name": "entitizer-free-api",
  "version": "0.1.0",
  "private": true,
  "description": "Entitizer free API",
  "main": "./lib/app.js",
  "scripts": {
    "start": "tsc && node ./lib/app.js",
    "build": "tsc",
    "pretest": "npm run clean && npm run tsc",
    "test": "ava --no-cache --verbose lib/**/*.test.js",
    "posttest": "npm run clean-test-code",
    "tsc": "tsc",
    "tscw": "tsc -w",
    "preversion": "npm run tsc",
    "postversion": "git push --follow-tags",
    "prepublishOnly": "npm run tsc && npm run clean-test-code",
    "clean": "rimraf ./lib && rimraf ./types",
    "clean-test-code": "rimraf ./lib/**/*.test.js && rimraf ./types/**/*.test.d.ts"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://Mitea@bitbucket.org/entitizer/free-api-js.git"
  },
  "keywords": [
    "entitizer",
    "textactor",
    "app",
    "free-api",
    "api"
  ],
  "author": "Dumitru Cantea",
  "license": "ISC",
  "bugs": {
    "url": "https://Mitea@bitbucket.org/entitizer/free-api-js/issues"
  },
  "homepage": "https://Mitea@bitbucket.org/entitizer/free-api-js#readme",
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/lru-cache": "^4.1.0",
    "@types/node": "^9.6.6",
    "ava": "^0.23.0",
    "debug": "^3.1.0",
    "dotenv": "^5.0.1",
    "rimraf": "^2.6.2",
    "typescript": "latest"
  },
  "dependencies": {
    "@textactor/actor-data": "^0.1.4",
    "@textactor/domain": "^0.1.6",
    "@textactor/ner": "^0.1.6",
    "@textactor/wikientity-data": "^0.1.2",
    "@textactor/wikientity-domain": "^0.1.2",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-rate-limit": "^2.11.0",
    "lru-cache": "^4.1.3",
    "ms": "^2.1.1",
    "ournet.logger": "^0.1.0"
  }
}
